<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bookstore.view.AddWriterDialogController">
   <children>
      <TextField fx:id="txtWriter" prefHeight="25.0" prefWidth="190.0" promptText="Writer's name" />
      <Button fx:id="buttonOK" mnemonicParsing="false" onAction="#uploadWriter" text="OK" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</HBox>
